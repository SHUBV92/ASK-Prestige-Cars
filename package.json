  "dependencies": {
    "axios": "^0.19.2",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "geocode": "0.0.3",
    "nodemailer": "^6.4.10",
    "nodemailer-smtp-transport": "^2.7.4",
    "nodemon": "^2.0.4",
    "react-geocode": "^0.2.1"
